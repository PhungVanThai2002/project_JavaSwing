        disPlays2();
        sum2();
    }                                  
    Connection con = null;
    PreparedStatement pst = null;
    ResultSet Rs = null,Rs1 = null;
    Statement St = null, St1 = null;
        private void disPlays(){
        String connectionUrl =  "jdbc:sqlserver://localhost;encrypt=false;user=sa;password=123456;applicationName=ADMIN-PC\\SQLEXPRESS;databaseName=QLtien";
        try {con = DriverManager.getConnection(connectionUrl);
            St = con.createStatement();
            Rs = St.executeQuery("select * from student ");
            thongketable.setModel(DbUtils.resultSetToTableModel(Rs));
        } catch (Exception e) {
        }
    }
        private void disPlays1(){
        String connectionUrl =  "jdbc:sqlserver://localhost;encrypt=false;user=sa;password=123456;applicationName=ADMIN-PC\\SQLEXPRESS;databaseName=QLtien";
        try {con = DriverManager.getConnection(connectionUrl);
            St = con.createStatement();
            Rs = St.executeQuery("select * from student where masv='"+masvtk.getText()+"'");
            thongketable.setModel(DbUtils.resultSetToTableModel(Rs));
        } catch (Exception e) {
        }
    }
        private void disPlays2(){
        String connectionUrl =  "jdbc:sqlserver://localhost;encrypt=false;user=sa;password=123456;applicationName=ADMIN-PC\\SQLEXPRESS;databaseName=QLtien";
        try {con = DriverManager.getConnection(connectionUrl);
            St = con.createStatement();
            Rs = St.executeQuery("select * from student where masv='"+masvtk.getText()+"'"+"AND hocky='"+hockitk.getSelectedItem().toString()+"'"+"AND namhoc='"+namhoctk.getSelectedItem().toString()+"'");
            thongketable.setModel(DbUtils.resultSetToTableModel(Rs));
        } catch (Exception e) {
        }
    }
        private void sum2(){
        String connectionUrl =  "jdbc:sqlserver://localhost;encrypt=false;user=sa;password=123456;applicationName=ADMIN-PC\\SQLEXPRESS;databaseName=QLtien";
        try {con = DriverManager.getConnection(connectionUrl);
            St = con.createStatement();
            Rs = St.executeQuery("select SUM(tiennop) from student where masv='"+masvtk.getText()+"'"+"AND hocky='"+hockitk.getSelectedItem().toString()+"'"+"AND namhoc='"+namhoctk.getSelectedItem().toString()+"'");
            table1tk.setModel(DbUtils.resultSetToTableModel(Rs));
        } catch (Exception e) {
        }
    }
            private void sum1(){
        String connectionUrl =  "jdbc:sqlserver://localhost;encrypt=false;user=sa;password=123456;applicationName=ADMIN-PC\\SQLEXPRESS;databaseName=QLtien";
        try {con = DriverManager.getConnection(connectionUrl);
            St = con.createStatement();
            Rs = St.executeQuery("select SUM(tiennop) from student where masv='"+masvtk.getText()+"'");
            table1tk.setModel(DbUtils.resultSetToTableModel(Rs));
        } catch (Exception e) {
        }
    }
